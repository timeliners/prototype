{
	"name": "timeliners",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "react-scripts-ts start",
		"build": "react-scripts-ts build",
		"eject": "react-scripts-ts eject",
		"test": "run-s test:*",
		"test:code": "react-scripts-ts test --env=jsdom --coverage",
		"test:compile": "tsc",
		"test:lint": "tslint src/**/*.{ts,tsx}",
		"test:security": "nsp check",
		"now": "run-s now:*",
		"now:deploy": "now deploy ./build -A ../now.json --team timeliners --token=$NOW_TOKEN",
		"now:alias": "now alias --team timeliners --token=$NOW_TOKEN",
		"now:clean": "now rm --team timeliners --token=$NOW_TOKEN prototype --safe --yes",
		"codecov": "codecov"
	},
	"dependencies": {
		"react": "^16.3.2",
		"react-dom": "^16.3.2"
	},
	"devDependencies": {
		"@types/chai": "^4.1.3",
		"@types/enzyme": "^3.1.10",
		"@types/jest": "^22.2.3",
		"@types/node": "^9.6.7",
		"@types/react": "^16.3.13",
		"@types/react-dom": "^16.0.5",
		"chai": "^4.1.2",
		"codecov": "^3.0.1",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"jest-enzyme": "^6.0.0",
		"now": "^11.1.7",
		"npm-run-all": "^4.1.2",
		"nsp": "^3.2.1",
		"react-scripts-ts": "^3.0.0",
		"tslint": "^5.9.1",
		"typescript": "^2.8.3"
	}
}
